<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="forum.jiangyouluntan.com.videocropdemo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".MyApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FFmpegAndroidLibraryActivity" />
        <activity android:name=".AndroidFFmpegLibraryActivity"/>
        <activity android:name=".FFmpegAndroidLibraryGetAllImageActivity"/>
        <activity android:name=".MediaCodecActivity"/>
        <activity android:name=".MediaMetadataRetrieverActivity"/>
        <activity android:name=".FFmpegMediaMetadataRetrieverActivity"/>
        <activity android:name=".MediaMetadataRetrieverVideoViewActivity"/>
        <activity android:name=".EndProjectActivity"/>
        <activity android:name=".CropVideoActivity"/>
        <activity android:name=".CropVideo2Activity"/>
    </application>

</manifest>